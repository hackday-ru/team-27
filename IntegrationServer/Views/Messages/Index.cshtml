@model IEnumerable<IntegrationServer.Models.Message>

@{
    ViewBag.Title = "Сообщения";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ReceiveTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DeviceSerial)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SignalId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EventTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProcessingFinish)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProcessingStart)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ReceiveTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DeviceSerial)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SignalId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EventTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProcessingFinish)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProcessingStart)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) 
           
        </td>
    </tr>
}

</table>
